<li>
  <a href="#/questions/<%= question.id %>">
    <%= question.escape('title') %>
  </a>

  <ul class="question-tags group">
    <% question.tags().each(function(tag) { %>
      <li>
        <a class="tag-link" href="#/tags/<%= tag.id %>">
          <%= tag.escape("tag_name") %>
        </a>
      </li>
    <% }); %>
  </ul>
</li>
